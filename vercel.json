{
  "rewrites": [
    { "source": "/((?!.*\\.(?:png|jpg|jpeg|gif|webp|svg|ico|css|js|map|json)).*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/og-image.png",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
        { "key": "Content-Type", "value": "image/png" }
      ]
    }
  ]
}
